{
  "entryFile": "src/index.ts",
  "noImplicitAdditionalProperties": "throw-on-extras",
  "controllerPathGlobs": ["src/controllers/*.ts"],
  "spec": {
    "outputDirectory": "build",
    "specVersion": 3
  },
  "swagger": {
    "outputDirectory": "src/swagger",
    "basePath": "/",
    "specVersion": 3
  },
  "routes": {
    "basePath": "/",
    "routesDir": "src/routes"
  }
}
